<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pgapp">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".PgAddListActivity"></activity>
        <activity android:name=".SelectCityActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ViewpaymentActivity" />
        <activity android:name=".SearchpropertyActivity" />
        <activity android:name=".MybookingActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity
            android:name=".SplashscreenActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".ResetpasswordActivity" />
        <activity android:name=".VerificationActivity" />
        <activity android:name=".ForgotpasswordActivity" />
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".RegistrationActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>